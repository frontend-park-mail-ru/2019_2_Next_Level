{
  "version": 2,
  "name": "Next Level Mail",
  "alias": ["next-level-mail"],
  "builds": [
    { "src": "package.json", "use": "@now/static-build" },
    { "src": "server/index.js", "use": "@now/node-server" },
    { "src": "public/**", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/", "dest": "public/index.html" },
    { "src": "/api/(.*)", "dest": "server/index.js" },
    { "src": "/(.*)", "dest": "public/$1" }
  ]
}
